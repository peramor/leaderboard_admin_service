{
  "$schema": "http://json-schema.org/draft-04/schema",
  "type": "object",
  "properties": {
    "receivers": {
      "type": "array",
      "items": {
        "type": "number",
        "description": "telegram ids"
      }
    },
    "messages": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "number"
          },
          "postTime": {
            "type": "number",
            "description": "timestamp in sec"
          },
          "content": {
            "type": "string"
          }
        }
      }
    }
  }
}